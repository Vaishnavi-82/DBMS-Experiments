SELECT ENAME, JOB, SAL
FROM EMPLOYEE
WHERE DEPTNO = 30
ORDER BY SAL DESC;
SELECT JOB, DEPTNO
FROM EMPLOYEE
WHERE ENAME LIKE 'A___N';
SELECT ENAME
FROM EMPLOYEE
WHERE ENAME LIKE 'S%';
SELECT ENAME
FROM EMPLOYEE
WHERE ENAME LIKE '%S';
SELECT ENAME
FROM EMPLOYEE
WHERE DEPTNO IN (10,20,40)
   OR JOB IN ('CLERK','SALESMAN','ANALYST');
SELECT EMPNO, ENAME
FROM EMPLOYEE
WHERE COMM IS NOT NULL
AND COMM > 0;
SELECT EMPNO,
       SAL + IFNULL(COMM,0) AS TOTAL_SALARY
FROM EMPLOYEE;
SELECT EMPNO,
       SAL * 12 AS ANNUAL_SALARY
FROM EMPLOYEE;
SELECT ENAME
FROM EMPLOYEE
WHERE JOB = 'CLERK'
AND SAL > 3000;
SELECT ENAME
FROM EMPLOYEE
WHERE JOB IN ('CLERK','SALESMAN','ANALYST')
AND SAL > 3000;
